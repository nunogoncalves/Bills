<ul class="people_list">
<% @dashboard_presenter.tenants.each do |tenant| %>
	<li style="background: <%= tenant.color %>" class="person" data-tenant_in="<%= tenant.entered_on %>" data-tenant_out="<%= tenant.really_exited_on %>" data-tenant_id="<%= tenant.id %>" data-name="<%= tenant.name %>">
		<div style="display: inline-block">
			<%#= image_tag "#{actors.sample}.jpg", id: "tentant_#{tenant.id}_image", style: 'width: 75px; height: 100px' %>
		</div>
			<div><%= tenant.name %></div>
			<div>numicago@gmail.com</div>
			<div>Entrou Sa√≠u</div>
			<div><%= tenant.entered_on %> <%= tenant.really_exited_on %></div>
			<div>Total a pagar: </div>
		</div>
		<div>
			<%#= image_tag "#{actors.sample}.jpg", id: "tentant_#{tenant.id}_image" %>
			<%= image_tag "https://dl.dropboxusercontent.com/u/2001692/actor/#{actors.sample}.jpg", id: "tentant_#{tenant.id}_image" %>
		</div>
	</li>
<% end %>

<% if @dashboard_presenter.tenants.count < house_max %>
	<li>
		<div onclick="newPersonInput(this)">+</div>
		<script>
			function newPersonInput(element) {
				var $element = $(element);
				$element.parent().html("<div>Foto Nome email entrou saiu</div>");
			}
		</script>
	</li>
<% end %>
</ul>
